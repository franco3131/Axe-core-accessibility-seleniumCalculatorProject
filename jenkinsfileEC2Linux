pipeline {
    agent {label 'slave'}
    
    stages{
    	stage('Hello') {
    		steps {
    			echo 'Hello world'
    		}
    	}
    	        stage ('Compile Stage') {

            steps {
                sh 'mvn clean install'
	
	 

            }
        }
    stage ('Test Stage') {

            steps {

                    sh 'mvn test'

                

            }
        }
    
     }
   }

